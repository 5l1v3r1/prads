---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Quelea</title>
 <link href="http://gamelinux.github.com/prads/atom.xml" rel="self"/>
 <link href="http://gamelinux.github.com/prads/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://gamelinux.github.com/prads/</id>
 <author>
   <name>prads</name>
   <email>email@foo.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://gamelinux.github.com/prads{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://gamelinux.github.com/prads{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
